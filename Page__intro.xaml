<Page x:Class="MyOwnGame_Multipage.Page__intro"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MyOwnGame_Multipage"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Своя игра">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="images\MyOwnGame_bg.jpg"/>
        </Grid.Background>



        <StackPanel  VerticalAlignment="Center">
            <Button Template="{StaticResource introBtn}" Name="btnIntroName" Click="btnIntroName_Click" />
        </StackPanel>

    </Grid>
</Page>
