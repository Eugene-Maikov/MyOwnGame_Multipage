<Window x:Class="MyOwnGame_Multipage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyOwnGame_Multipage"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="MainFrame" Background="#fff" NavigationUIVisibility="Hidden"/>
    </Grid>

    <Window.Resources>
        <ControlTemplate TargetType="Button" x:Key="introBtn">
            <Border CornerRadius="20"></Border>
        </ControlTemplate>
    </Window.Resources>
</Window>
